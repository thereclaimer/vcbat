{
    "version": "2.0.0",
    "tasks": [
		{
			"label": "Build Win32 VCBat",
			"command": "scripts\\build-win32.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
			}
		},
		{
			"label": "Build Tools",
			"command": "scripts\\build-win32-tools.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
			}
		},
		{
			"label": "Build UI Font",
			"command": "scripts\\build-font-ui.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": {
				"kind": "build",
			}
		}
	]
} 